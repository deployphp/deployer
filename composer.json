{
    "name": "deployer/deployer",
    "description": "Deployment Tool",
    "license": "MIT",
    "homepage": "https://deployer.org",
    "support": {
        "docs": "https://deployer.org/docs",
        "source": "https://github.com/deployphp/deployer",
        "issues": "https://github.com/deployphp/deployer/issues"
    },
    "authors": [
        {
            "name": "Anton Medvedev",
            "email": "anton@medv.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Deployer\\": "src/"
        },
        "files": [
            "src/Support/helpers.php",
            "src/functions.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": [
        "bin/dep"
    ],
    "require": {
        "php": "^7.3",
        "pimple/pimple": "^3",
        "symfony/console": "^5",
        "symfony/process": "^5",
        "symfony/yaml": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "config": {
        "sort-packages": true
    }
}
